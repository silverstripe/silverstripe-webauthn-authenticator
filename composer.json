{
    "name": "silverstripe/webauthn-authenticator",
    "description": "A WebAuthn authenticator that plugs into the silverstripe/mfa module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.1",
        "ext-bcmath": "*",
        "silverstripe/framework": "^5",
        "silverstripe/mfa": "^5",
        "web-auth/webauthn-lib": "^4.4",
        "guzzlehttp/psr7": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\WebAuthn\\": "src/",
            "SilverStripe\\WebAuthn\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
