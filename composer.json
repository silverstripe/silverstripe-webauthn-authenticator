{
    "name": "silverstripe/webauthn-authenticator",
    "description": "A WebAuthn authenticator that plugs into the silverstripe/mfa module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/mfa": "^4.0",
        "web-auth/webauthn-lib": "^3.3",
        "guzzlehttp/psr7": "^1.6"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\WebAuthn\\": "src/",
            "SilverStripe\\WebAuthn\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
