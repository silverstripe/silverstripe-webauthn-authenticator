{
    "name": "silverstripe/webauthn-authenticator",
    "description": "A WebAuthn authenticator that plugs into the silverstripe/mfa module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-bcmath": "*",
        "silverstripe/framework": "^4.12",
        "silverstripe/mfa": "^4.6",
        "web-auth/webauthn-lib": "^3.3",
        "guzzlehttp/psr7": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\WebAuthn\\": "src/",
            "SilverStripe\\WebAuthn\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist",
            "client/lang"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
